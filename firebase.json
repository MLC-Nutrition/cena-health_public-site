{
	"hosting": {
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		],
		"target": "public-site"
	},
	"emulators": {
		"hosting": {
			"port": 5000
		},
		"ui": {
			"enabled": true,
			"port": 8181
		},
		"singleProjectMode": true
	}
}
